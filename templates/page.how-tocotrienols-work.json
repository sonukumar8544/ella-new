    "howt_tocotrienole_works_teo_col_46w8U8": {
      "type": "howt-tocotrienole-works-teo-col",
      "blocks": {
        "content_block_YU7k9F": {
          "type": "content_block",
          "settings": {
            "block_title": "Superior Cell Penetration",
            "paragraph_1": "Their unsaturated side chain makes tocotrienols smaller and more flexible, helping them slip deep into your cell membranes — more efficiently than tocopherols.",
            "paragraph_1_italic": false,
            "paragraph_2": "<style>\n  .how-toto-work-title {\n    font-size: 18px;\n    font-weight: 600;\n    margin-bottom: 12px;\n   font-style: italic;\n  color: #374650;\n  }\n\n  .how-toto-work-list {\n    padding-left: 18px;\n    margin: 0;\n  }\n\n  .how-toto-work-item {\n    font-size: 15px;\n    line-height: 1.6;\n    margin-bottom: 8px;\ncolor: #374650;\n  }\n</style>\n\n<div class=\"how-toto-work-wrapper\">\n  <div class=\"how-toto-work-title\">What this means for you</div>\n  <ul class=\"how-toto-work-list\">\n    <li class=\"how-toto-work-item\">Protection at precise areas where oxidative stress begins</li>\n    <li class=\"how-toto-work-item\">Deeper, more meaningful cellular protection</li>\n    <li class=\"how-toto-work-item\">More effective shielding of sensitive tissues</li>\n  </ul>\n</div>",
            "paragraph_2_italic": true,
            "paragraph_2_bg_enable": false,
            "paragraph_3": "",
            "paragraph_3_italic": true,
            "paragraph_3_bg_enable": false,
            "paragraph_4": "",
