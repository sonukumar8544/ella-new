    "color_driven_faq_section_LGqPXx": {
      "type": "color-driven-faq-section",
      "blocks": {
        "faq_item_6dGYEX": {
          "type": "faq_item",
          "settings": {
            "question": "Why haven't I heard of tocotrienols?",
            "answer": "Most vitamin E research and marketing have focused on alpha-tocopherol, which was discovered first. Tocotrienols were only isolated in the late 1980s and their superior benefits have been overshadowed by the more common form. The supplement industry has been slow to adopt this advanced form due to higher sourcing costs.",
            "open_by_default": true
          }
        },
        "faq_item_6nJKM8": {
          "type": "faq_item",
          "settings": {
            "question": "Are tocotrienols safe?",
            "answer": "Yes, tocotrienols have an excellent safety profile. They are naturally occurring nutrients found in certain foods. Clinical studies have shown no adverse effects at doses up to 400mg daily. Our 300mg dose is approved by Health Canada for safety and efficacy.",
            "open_by_default": false
          }
        },
        "faq_item_8hccPN": {
          "type": "faq_item",
          "settings": {
            "question": "Why are tocotrienols costlier than tocopherols?",
            "answer": "Tocotrienols are much rarer in nature than tocopherols. While tocopherols can be cheaply synthesized, high-quality tocotrienols must be carefully extracted from specific plant sources like annatto. The extraction and purification process is more complex and expensive, but delivers a significantly more potent form of vitamin E.",
            "open_by_default": false
          }
        },
        "faq_item_m4jMF7": {
          "type": "faq_item",
          "settings": {
            "question": "How soon will I notice benefits?",
            "answer": "Most users report noticeable improvements in skin appearance and energy levels within 2-4 weeks. Cardiovascular and metabolic benefits typically become measurable after 8-12 weeks of consistent use. For optimal results, we recommend taking tocotrienols daily with a meal containing some healthy fats to enhance absorption.",
            "open_by_default": false
          }
        }
      },
      "block_order": [
        "faq_item_6dGYEX",
        "faq_item_6nJKM8",
        "faq_item_8hccPN",
