            class="health_benefits_img"
            src="{{ benefit.benefit_imageurl_1.value }}"
            alt="{{ benefit.benefit_imageurl_content_1.value }}"
            width=""
            height=""
            loading="lazy"
          >
        </span>
        {% endif %}
        <span class="content_benefits">{{ benefit.benefit_imageurl_content_1.value }}</span>
      </div>
