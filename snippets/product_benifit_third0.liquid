{% style %}
  .health-benefits_thirdO {
    display: flex;
    flex-wrap: wrap;
    gap: 24px;
    margin: 20px auto;
  }

  .health-benefits_thirdO .benefit-item {
    display: flex;
    align-items: center;
    gap: 7px;
    width: 30%;
    font-size: 16px;
    color: #2c2c2c;
