                  >
                    {% for i in (1..8) %}
                      {% capture image %}image_{{ i }}{% endcapture %}
                      {% capture show_image %}show_image_{{ i }}{% endcapture %}
                      {% capture link %}link_{{ i }}{% endcapture %}

                      {% capture product_1 %}lb_product_{{ i }}_1{% endcapture %}
                      {% capture left_1 %}horizontal_pos_{{ i }}_1{% endcapture %}
                      {% capture top_1 %}vertical_pos_{{ i }}_1{% endcapture %}
                      {% capture lookbook_pos_1 %}lookbook_pos_{{ i }}_1{% endcapture %}

                      {% capture product_2 %}lb_product_{{ i }}_2{% endcapture %}
                      {% capture left_2 %}horizontal_pos_{{ i }}_2{% endcapture %}
                      {% capture top_2 %}vertical_pos_{{ i }}_2{% endcapture %}
                      {% capture lookbook_pos_2 %}lookbook_pos_{{ i }}_2{% endcapture %}

                      {%- if block.settings[show_image] -%}
                        <div class="halo-item">
                          <div
                            class="imgGallery-item lookbook-image-item {% if enable_lookbook_width %}width-restricted{% endif %}"
                            data-lookbook-item-container
                            style="
                              --dot-desktop-size: {{ dot_desktop_size }};
                              --dot-mobile-size: {{ dot_mobile_size }};
                              --dot-shadow-color: {{ dot_shadow_color }};
                              --dot-outer-color: {{ dot_outer_color }};
                              --dot-inner-color: {{ dot_inner_color }};
                              --dot-number-desktop-size: {{ number_desktop_size }};
                              --dot-number-mobile-size: {{ number_mobile_size }};
                              --max-width: {{ lookbook_width }};
                              --des-max-width: {% if des_width > 0 %}{{ des_width | append: 'px' }}{% else %}unset{% endif %};
                              --truncate-line: {{ lookbook_truncate }};
                            "
                          >
                            <div class="img-box">
                              {% if block.settings[image] != blank %}
                                {%- assign preview_img = block.settings[image] -%}
                                <a
                                  {% if type_image_gallery == 'link' %}
                                    {% if block.settings[link] == blank %}
                                      role="link" aria-disabled="true"
                                    {%- else %}
                                      href="{{ block.settings[link] }}"
                                    {%- endif %}
                                    class="image-zoom adaptive_height"
                                  {% else %}
                                    href="{{ preview_img | img_url: 'master' }}" data-fancybox="images"
                                    class="image-zoom adaptive_height fancybox"
                                  {% endif %}
                                  style="padding-top: {{ 1 | divided_by: preview_img.aspect_ratio | times: 100 }}%"
                                >
                                  <img {% render 'lazyload', preview_img: preview_img %}>
                                  {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
                                </a>
                              {% else %}
                                <a
                                  {% if block.settings[link] == blank %}
                                    role="link" aria-disabled="true"
                                  {% else %}
                                    href="{{ block.settings[link] }}"
                                  {% endif %}
                                  class="image-zoom"
                                >
                                  <div class="not_img" style="--height_not_img: 370px">290 x 370px</div>
                                </a>
                              {% endif %}
                            </div>
                            {% if block.settings[product_1] != blank or block.settings[product_2] != blank %}
                              <lookbook-wrapper>
                                {% if block.settings[product_1] != blank %}
                                  {% assign top = block.settings[top_1] | append: '%' %}
                                  {% assign left = block.settings[left_1] | append: '%' %}

                                  {% render 'lookbook-modal-wrapper',
                                    lookbook_style: lookbook_style,
                                    top: top,
                                    left: left,
                                    show_dot_number: show_dot_number,
                                    show_dot_number_mobile: show_dot_number_mobile,
                                    lookbook_pos: block.settings[lookbook_pos_1],
                                    product: block.settings[product_1],
                                    show_plus_icon: show_plus_icon,
                                    num: 1
                                  %}
                                {% endif %}
                                {% if block.settings[product_2] != blank %}
                                  {% assign top = block.settings[top_2] | append: '%' %}
                                  {% assign left = block.settings[left_2] | append: '%' %}

                                  {% render 'lookbook-modal-wrapper',
                                    lookbook_style: lookbook_style,
                                    top: top,
                                    left: left,
                                    show_dot_number: show_dot_number,
                                    show_dot_number_mobile: show_dot_number_mobile,
                                    lookbook_pos: block.settings[lookbook_pos_2],
                                    product: block.settings[product_2],
                                    show_plus_icon: show_plus_icon,
                                    num: 2
                                  %}
                                {% endif %}
                              </lookbook-wrapper>
                            {% endif %}
                          </div>
                        </div>
                      {%- endif -%}
                    {% endfor %}
                  </div>
                </div>
              {%- elsif block.type == 'lookbook_banner' -%}
                {%- liquid
                  assign image = block.settings.image
                  assign preview_img = block.settings.image
                  assign margin_bottom = block.settings.margin_bottom

                  assign content_max_width = block.settings.content_max_width | append: 'px'

                  assign title = block.settings.title_text
                  assign title_fs = block.settings.title_fs | append: 'px'
                  assign title_fs_mb = block.settings.title_fs_mb | append: 'px'
                  assign title_color = block.settings.title_color
                  assign title_fw = block.settings.title_fw
                  assign title_margin_bottom = block.settings.title_margin_bottom | append: 'px'

                  assign button_1_text = block.settings.button_1_text
                  assign button_1_text_color = block.settings.button_1_text_color
                  assign button_1_background_color = block.settings.button_1_background_color
                  assign button_1_background_gradient_color = block.settings.button_1_background_gradient_color
                  assign button_1_border_color = block.settings.button_1_border_color
                  assign button_1_text_color_hover = block.settings.button_1_text_color_hover
                  assign button_1_background_color_hover = block.settings.button_1_background_color_hover
                  assign button_1_background_gradient_color_hover = block.settings.button_1_background_gradient_color_hover
                  assign button_1_border_color_hover = block.settings.button_1_border_color_hover
                  assign button_1_width = block.settings.button_1_width | append: 'px'
                  assign button_1_fs = block.settings.button_1_fs | append: 'px'
                  assign button_1_fs_mb = block.settings.button_1_fs_mb | append: 'px'
                  assign button_1_fw = block.settings.button_1_fw
                  assign link_1 = block.settings.link_1

                  assign button_2_text = block.settings.button_2_text
                  assign button_2_text_color = block.settings.button_2_text_color
                  assign button_2_background_color = block.settings.button_2_background_color
                  assign button_2_background_gradient_color = block.settings.button_2_background_gradient_color
                  assign button_2_border_color = block.settings.button_2_border_color
                  assign button_2_text_color_hover = block.settings.button_2_text_color_hover
                  assign button_2_background_color_hover = block.settings.button_2_background_color_hover
                  assign button_2_background_gradient_color_hover = block.settings.button_2_background_gradient_color_hover
                  assign button_2_border_color_hover = block.settings.button_2_border_color_hover
                  assign button_2_width = block.settings.button_2_width | append: 'px'
                  assign button_2_fs = block.settings.button_2_fs | append: 'px'
                  assign button_2_fs_mb = block.settings.button_2_fs_mb | append: 'px'
                  assign button_2_fw = block.settings.button_2_fw
                  assign link_2 = block.settings.link_2
                -%}

                <div
                  class="articleLookbook-block{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}"
                  style="--lookbook-margin-bottom: {{ margin_bottom | append: 'px' }}"
                  ;
                >
                  <div class="articleLookbook-item">
                    {%- if image != blank -%}
                      <a
                        class="image image-zoom image-adapt adaptive_height"
                        href="javascript:void(0)"
                        style="padding-top: {{ 1 | divided_by: image.aspect_ratio | times: 100 | append: '%' }}"
                      >
                        <img {% render 'lazyload', preview_img: preview_img %}>
                        {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
                      </a>
                    {%- else -%}
                      <a class="image" href="javascript:void(0)">
                        <div class="not_img" style="--height_not_img: 520px">930 x 520px</div>
                      </a>
                    {%- endif -%}
                    {% if title != blank or button_1_text != blank or button_2_text != blank %}
                      <div
                        class="articleLookbook-item-content text-center"
                        style="--max-width: {{ content_max_width }};"
                      >
                        {% if title != blank %}
                          <h3
                            class="lookbook-banner-title text-center"
                            style="
                              --title-color: {% if title_color != blank and title_color != 'rgba(0,0,0,0)' %}{{ title_color }}{% endif %};
                              --title-fs: {{ title_fs }};
                              --title-fs-mb: {{ title_fs_mb }};
                              --title-fw: {{ title_fw }};
                              --title-mg-bottom: {{ title_margin_bottom }};
                            "
                          >
                            <span class="text">
                              {{ title }}
                            </span>
                          </h3>
                        {% endif %}
                        {% if button_1_text != blank or button_2_text != blank %}
                          <div class="lookbook-banner-btns-wrapper">
                            {% if button_1_text != blank %}
                              <a
                                class="button"
                                role="button"
                                {% if link_1 == blank %}
                                  role="link" aria-disabled="true"
                                {% else %}
                                  href="{{ link_1 }}"
                                {% endif %}
                                style="
                                  --btn-width: {{ button_1_width }};
                                  --btn-fs: {{ button_1_fs }};
                                  --btn-fs-mb: {{ button_1_fs_mb }};
                                  --btn-fw: {{ button_1_fw }};
                                  --btn-text-color: {% if button_1_text_color != blank and button_1_text_color != 'rgba(0,0,0,0)' %}{{ button_1_text_color }}{% endif %};
                                  --btn-border-color: {% if button_1_border_color != blank and button_1_border_color != 'rgba(0,0,0,0)' %}{{ button_1_border_color }}{% endif %};
                                  --btn-bg: {% if button_1_background_gradient_color != blank and button_1_background_gradient_color != 'rgba(0,0,0,0)' %}{{ button_1_background_gradient_color }}{% elsif button_1_background_color != blank and button_1_background_color != 'rgba(0,0,0,0)' %}{{ button_1_background_color }}{% endif %};
                                  --btn-text-hover-color: {% if button_1_text_color_hover != blank and button_1_text_color_hover != 'rgba(0,0,0,0)' %}{{ button_1_text_color_hover }}{% endif %};
                                  --btn-border-hover-color: {% if button_1_border_color_hover != blank and button_1_border_color_hover != 'rgba(0,0,0,0)' %}{{ button_1_border_color_hover }}{% endif %};
                                  --btn-hover-bg: {% if button_1_background_gradient_color_hover != blank and button_1_background_gradient_color_hover != 'rgba(0,0,0,0)' %}{{ button_1_background_gradient_color_hover }}{% elsif button_1_background_color_hover != blank and button_1_background_color_hover != 'rgba(0,0,0,0)' %}{{ button_1_background_color_hover }}{% endif %};
                                "
                              >
                                {{ button_1_text }}
                              </a>
                            {% endif %}
                            {% if button_2_text != blank %}
                              <a
                                class="button"
                                role="button"
                                {% if link_2 == blank %}
                                  role="link" aria-disabled="true"
                                {% else %}
                                  href="{{ link_2 }}"
                                {% endif %}
                                style="
                                  --btn-width: {{ button_2_width }};
                                  --btn-fs: {{ button_2_fs }};
                                  --btn-fs-mb: {{ button_2_fs_mb }};
                                  --btn-fw: {{ button_2_fw }};
                                  --btn-text-color: {% if button_2_text_color != blank and button_2_text_color != 'rgba(0,0,0,0)' %}{{ button_2_text_color }}{% endif %};
                                  --btn-border-color: {% if button_2_border_color != blank and button_2_border_color != 'rgba(0,0,0,0)' %}{{ button_2_border_color }}{% endif %};
                                  --btn-bg: {% if button_2_background_gradient_color != blank and button_2_background_gradient_color != 'rgba(0,0,0,0)' %}{{ button_2_background_gradient_color }}{% elsif button_2_background_color != blank and button_2_background_color != 'rgba(0,0,0,0)' %}{{ button_2_background_color }}{% endif %};
                                  --btn-text-hover-color: {% if button_2_text_color_hover != blank and button_2_text_color_hover != 'rgba(0,0,0,0)' %}{{ button_2_text_color_hover }}{% endif %};
                                  --btn-border-hover-color: {% if button_2_border_color_hover != blank and button_2_border_color_hover != 'rgba(0,0,0,0)' %}{{ button_2_border_color_hover }}{% endif %};
                                  --btn-hover-bg: {% if button_2_background_gradient_color_hover != blank and button_2_background_gradient_color_hover != 'rgba(0,0,0,0)' %}{{ button_2_background_gradient_color_hover }}{% elsif button_2_background_color_hover != blank and button_2_background_color_hover != 'rgba(0,0,0,0)' %}{{ button_2_background_color_hover }}{% endif %};
                                "
                              >
                                {{ button_2_text }}
                              </a>
                            {% endif %}
                          </div>
                        {% endif %}
                      </div>
                    {% endif %}
                  </div>
                </div>
              {%- elsif block.type == 'block_quote' -%}
                {%- liquid
                  assign enable_bg_color = block.settings.enable_bg_color
                  assign bg_color_content = block.settings.bg_color_content
                  assign title = block.settings.title
                  assign color_title = block.settings.color_title
                  assign fontsize_title = block.settings.fontsize_title
                  assign margin_bottom_title = block.settings.margin_bottom_title
                  assign content = block.settings.content
                  assign color_content = block.settings.color_content
                  assign fontsize_content = block.settings.fontsize_content
                  assign margin_bottom = block.settings.margin_bottom
                -%}
                <div
                  class="articleBlockQuote{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}"
                  style="--quote-margin-bottom: {{ margin_bottom | append: 'px' }}"
                  ;
                >
                  <div
                    class="articleBlockQuote-header"
                    style="
                      --quote-color-title: {{ color_content }};
                      --quote-fontsize-title: {{ fontsize_title | append: 'px' }};
                      --quote-margin-bottom-title: {{ margin_bottom_title | append: 'px' }}
                    "
                  >
                    <h3 class="title">{{ title }}</h3>
                  </div>
                  <div
                    class="articleBlockQuote-content{% if enable_bg_color %} has-bg{% endif %}"
                    style="
                      --quote-color-content: {{ color_content }};
                      --quote-bg-color-content: {{ bg_color_content }};
                      --quote-fontsize-content: {{ fontsize_content | append: 'px' }}
                    "
                  >
                    {{ content }}
                  </div>
                </div>
              {%- elsif block.type == 'product_block' -%}
                {% assign position = block.settings.position %}
                {% if position == 'content' %}
                  {% render 'halo-side-product-block',
                    block: block,
                    show_collapse: false,
                    sidebar_collapse: sidebar_collapse_default,
                    position: position
                  %}
                {% endif %}
              {%- endif -%}
            {%- endfor -%}
            {%- if show_tags -%}
              {%- if article.tags.size > 0 -%}
                <div class="article-tag{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
                  <div class="article-tag-row">
                    <div class="tag-header">
                      {% render 'icon-tag-solid' %}
                      <span>{{ 'blogs.article.tags' | t }}</span>
                    </div>
                    <ul class="tag-list list-unstyled clearfix">
                      {%- for tag in article.tags -%}
                        <li class="item">
                          <a href="{{ blog.url }}/tagged/{{ tag | handle }}">
                            <span class="text">{{ tag }}</span>
                          </a>
                        </li>
                      {%- endfor -%}
                    </ul>
                  </div>
                </div>
              {%- endif -%}
            {%- endif -%}
            {%- if show_share -%}
              <div
                id="share-indivisual-blog"
                class="article-social text-left{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}"
              >
                {% comment %} {% render 'halo-share', product: article %} {% endcomment %}
                {% render 'halo-social-media-icons' %}
              </div>
            {%- endif -%}
          </div>
        </div>
        {%- if show_next_prev -%}
          {%- if blog.next_article or blog.previous_article -%}
            <div class="article__pagination{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
              <div class="article__pagination--item article__pagination--prev">
                <a
                  class="button button--secondary button--prev"
                  href="{{ blog.previous_article }}"
                  {% unless blog.previous_article %}
                    disabled
                  {% endunless %}
                >
                  <span class="text"> {{ 'blogs.article.prev_post' | t }}</span>
                </a>
              </div>
              <div class="article__pagination--item article__pagination--next">
                <a
                  class="button button--secondary button--next"
                  href="{{ blog.next_article }}"
                  {% unless blog.next_article %}
                    disabled
                  {% endunless %}
                >
                  <span class="text">{{ 'blogs.article.next_post' | t }} </span>
                </a>
              </div>
            </div>
          {%- endif -%}
        {%- endif -%}
        {%- if blog.comments_enabled? -%}
          <div class="article-template__comment-wrapper background-secondary{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
            <div id="comments" class="page-width page-width--narrow">
              {%- if article.comments_count > 0 -%}
                {%- assign anchorId = '#Comments-' | append: article.id -%}
                <div class="halo-block-header">
                  <h3 id="Comments-{{ article.id }}" class="title text-left">
                    <span class="text">
                      {{ 'blogs.article.comments' | t: count: article.comments_count | minus: 1 }}
                    </span>
                  </h3>
                </div>
                {%- paginate article.comments by 5 -%}
                  <div class="sonu1111 article-template__comments">
                    {%- if comment.status == 'pending' and comment.content -%}
                      <article id="{{ comment.id }}" class="article-template__comments-comment">
                        <h4 class="comment-author">
                          {{ comment.author }}
                        </h4>
                        <div class="comment-body {{ comment.status }}">
                          {{ comment.content }}
                        </div>
                      </article>
                    {%- endif -%}

                    {%- for comment in article.comments -%}
                      {%- unless forloop.first -%}
                        <article id="{{ comment.id }}" class="article-template__comments-comment">
                          <div class="comment-user">
                            {% render 'icon-account-2' %}
                          </div>
                          <div class="comment-content">
                            <div class="comment-created-at">
                              <span class="comment-date">{{ comment.created_at | date: '%d %b %Y' }}</span>
                              <span class="comment-author">{{ comment.author }}</span>
                            </div>
                            <div class="comment-body {{ comment.status }}">
                              {{ comment.content }}
                            </div>
                          </div>
                        </article>
                      {%- endunless -%}
                    {%- endfor -%}

                    {% render 'pagination',
                      paginate: paginate,
                      class: 'order-pagination pagination-wrapper text-center',
                      anchor: anchorId
                    %}
                  </div>
                {%- endpaginate -%}
              {%- endif -%}
              {%- form 'new_comment', article -%}
                {%- liquid
                  assign post_message = 'blogs.article.success'
                  if blog.moderated? and comment.status == 'unapproved'
                    assign post_message = 'blogs.article.success_moderated'
                  endif
                -%}
                <div class="halo-block-header">
                  <h3 class="title text-left">
                    <span class="text">
                      {{ 'blogs.article.comment_form_title' | t }}
                    </span>
                  </h3>
                </div>
                {%- if form.errors -%}
                  {%- if form.errors -%}
                    <div class="form-message" role="alert">
                      <div class="alertBox alertBox--error">
                        <div class="alertBox-message">
                          <ul>
                            {%- for field in form.errors -%}
                              <li>
                                <a href="#CommentForm-{{ field }}" class="link">
                                  {%- if form.errors.translated_fields[field] contains 'author' -%}
                                    {{ 'blogs.article.name' | t }}
                                  {%- elsif form.errors.translated_fields[field] contains 'body' -%}
                                    {{ 'blogs.article.message' | t }}
                                  {%- else -%}
                                    {{ form.errors.translated_fields[field] }}
                                  {%- endif -%}
                                  {{ form.errors.messages[field] }}
                                </a>
                              </li>
                            {%- endfor -%}
                          </ul>
                        </div>
                      </div>
                    </div>
                  {%- endif -%}
                {%- elsif form.posted_successfully? -%}
                  <div class="form-message" role="status">
                    <div class="alertBox alertBox--success">
                      <p class="alertBox-message">
                        {{ post_message | t }}
                      </p>
                    </div>
                  </div>
                {%- endif -%}
                <div
                  {% if blog.moderated? == false %}
                    class="article-template__comments-fields"
                  {% endif %}
                >
                  <div class="article-template__comment-fields form-field-group clearfix">
