{% comment %}
  <div id="image-wrapper-ind-blogs">
  <div class="singleblg-page-banner-block-section">
  <div class="singleblg-page-banner-block-wrapper">
  <div class="singleblg-page-banner-block-left-text-section">
  <h1>{{ article.title }}</h1>
  </div>
  <div class="blocks-small-banner">
  {% if article.image %}
