<!-- main-article -->
<article
  id="blog_page_single"
  data-section-id="{{ section.id }}"
  data-section-type="article-template"
  id="ArticleSection-{{ section.id }}"
  data-section="{{ section.id }}"
  class="article-template"
  itemscope
  itemtype="http://schema.org/BlogPosting"
>
  <div class="{% if container == '1170' %}container-1170{% elsif container == '1770' %}container-1770{% elsif container == 'fullwidth'%}container-full{% else %}container{% endif %}">
    {%- if show_breadcrumb -%}
      {% render 'breadcrumb', alignment: breadcrumb_alignment %}
    {%- endif -%}
    {%- if show_sidebar -%}
      <div
        class="header-with-sidebar{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}"
      >
    {% endif %}
    {%- if show_page_title -%}
      <h1 class="page-header text-{{ page_title_alignment }}">
        {{ blog.title | escape }}
      </h1>
    {%- endif -%}
    {%- if show_sidebar and section.blocks.size > 0 -%}
      <div class="blog-filter-mobile-details" data-sidebar>
        <button class="filter-btn-mob-details">Recent Posts</button>
      </div>
      {% comment %}
        <div class="sonu-1 sidebar-toogle" data-sidebar>
        {% render 'icon-sidebar' %}
        </div>
      {% endcomment %}
    {%- endif -%}
    {% if show_sidebar %}
      </div>
    {% endif %}
    <div class="we-blog-title">
      <h1>{{ article.title }}</h1>
    </div>
    <div class="halo-article-block">
      {%- if show_sidebar and section.blocks.size > 0 -%}
        {{ 'component-main-sidebar.css' | asset_url | stylesheet_tag }}
        <div
          class="page-sidebar page-sidebar-{{ sidebar_position }} page-sidebar--vertical sidebar--{{ section.settings.sidebar_layout }}{% if enable_sticky %} has-sticky{% endif %}"
          id="halo-sidebar"
        >
          <a href="javascript:void(0)" class="halo-sidebar-close" data-close-sidebar role="button">
            <span class="visually-hidden">{{ 'general.common.close' | t }}</span>
            {% render 'icon-close' %}
          </a>
          <div class="halo-sidebar-header text-left">
            <span class="title">
              {{ 'sections.sidebar.title' | t }}
            </span>
          </div>
          <div class="halo-sidebar-wrapper custom-scrollbar{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
            {% render 'sidebar',
              section: section,
              show_collapse: show_collapse,
              sidebar_collapse: sidebar_collapse_default
            %}
          </div>
        </div>
      {%- endif -%}
      <div class="page-content">
        <div id="we-blog-banner">
          <div class="we-blog-image">
            <img
              class="desktop_view_blog lazyload"
              data-src="{{ article.image | img_url: 'master'}}"
              src="{{ article.image | img_url: '300x'}}"
              alt="{{ article.title }}"
              title=""
              width="100%"
              height="100%"
              style="width:auto; margin:auto;"
              loading="lazy"
            >
          </div>
        </div>

        <style>
          .we-blog-image img{
          width: 100%;
          height: auto;
          object-fit: cover;
          object-position: center;
          border-radius: 10px;
          margin-bottom: 20px;

          }

          .we-blog-title h1{
          font-size: 32px;
          font-weight: 700;
          color: #006a91;
          margin-bottom: 10px;
          line-height: 1.2;
          font-weight: 800;
          }
          .we-blog-title{
          max-width: 950px;
          padding-bottom: 10px;
          }

          div#we-blog-banner {
          margin-bottom: 25px;
          max-width: 950px;
          }

          @media (max-width: 768px) {
          .we-blog-title h1{
          font-size: 20px;
          margin-bottom: 10px;
          }
          div#we-blog-banner {
          margin-top: 15px;
          }
          }
        </style>
        <div class="halo-article-content{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
          <div class="article-header">
            <div class="author-top">
              {% assign blogauthor = article.author %}
              <div class="blog-author-details">
                {% if blogauthor == 'Sarika Jassal'
                  or blogauthor == 'Divya Katyal'
                  or blogauthor == 'Pragya Soni'
                  or blogauthor == 'Pooja Rawat'
                  or blogauthor == 'Aditi bakshi'
                  or blogauthor == 'Nurah Ekhlaque'
                %}
                  <h3>
                    <a href="https://wellnessextract.com/pages/{{ article.author | replace: ' ', '-' | downcase }}">
                      {% if article.author == 'Aditi bakshi' %}
                        <span><i>Author: </i>Dr. {{ article.author }}</span>
                        <span class="blog-author">
                           {% comment %} <span class="by-auth">Author</span> {% endcomment %}
                         </span>
                       {% else %}
                         <span class="blog-author">
                           <span><i>Author: </i>{{ article.author }}</span>
                           {% comment %} <span class="by-auth">Author</span> {% endcomment %}
                         </span>
                       {% endif %}

                    </a>
                  </h3>
                  {% if blogauthor == 'Sarika Jassal' %}
                    <img
                      class="lazyload"
                      data-src="https://cdn.shopify.com/s/files/1/0735/8172/3944/files/Sarika_40x.jpg?v=1737525344"
                      width="40px"
                      height="40px"
                      title=""
                      alt="sarika jassal"
                      loading="lazy"
                    >
                  {% elsif blogauthor == 'Divya Katyal' %}
                    <img
                      class="lazyload"
                      data-src="https://cdn.shopify.com/s/files/1/0735/8172/3944/files/Divya_40x.jpg?v=1737525344"
                      width="40px"
                      height="40px"
                      title=""
                      alt="divya"
                      loading="lazy"
                    >
                  {% elsif blogauthor == 'Pragya Soni' %}
                    <img
                      class="lazyload"
                      data-src="https://cdn.shopify.com/s/files/1/0735/8172/3944/files/Pragya_40x.jpg?v=1738648479"
                      width="40px"
                      height="40px"
                      title=""
                      alt="divya"
                      loading="lazy"
                    >
                  {% elsif blogauthor == 'Pooja Rawat' %}
                    <img
                      class="lazyload"
                      data-src="https://cdn.shopify.com/s/files/1/0735/8172/3944/files/Pooja-2_40x.jpg?v=1739511048"
                      width="40px"
                      height="40px"
                      title=""
                      alt="divya"
                      loading="lazy"
                    >
                  {% elsif blogauthor == 'Aditi bakshi' %}
                    <img
                      class="lazyload"
                      data-src="https://cdn.shopify.com/s/files/1/0735/8172/3944/files/Dr._Aditi_Bakshi_Msc-Nutrition_50x50_1.jpg?v=1748929087"
                      width="40px"
                      height="40px"
                      title=""
                      alt="divya"
                      loading="lazy"
                    >
                {% elsif blogauthor == 'Nurah Ekhlaque' %}
                    <img
                      class="lazyload"
                      data-src="https://wellnessextract.com/cdn/shop/files/nurah.jpg?v=1745843377"
                      width="40px"
                      height="40px"
                      title=""
                      alt="divya"
                      loading="lazy"
                    >

                  {% else %}
                    <img
                      class="lazyload"
                      data-src="https://cdn.shopify.com/s/files/1/0735/8172/3944/files/image_5_7935c512-a1c4-45fd-baaa-b6063fd0ed2c_40x.png?v=1738731314"
                      width="40px"
                      alt="divya"
                      height="40px"
                      title=""
                      loading="lazy"
                    >
                  {% endif %}
                {% else %}
                  <h3>
                    <span class="blog-author">
                      <span><i>By </i>{{ article.author }}</span>
                      <span class="by-auth">Author</span>
                    </span>
                  </h3>
                {% endif %}
              </div>
              {% for tag in article.tags %}
                {% if tag == 'felicia' %}
                  <div class="blog-editor">
                    <img
                      class="lazyload"
                      data-src="https://wellnessextract.com/cdn/shop/files/Felicia_300x300.jpg?v=1737439340"
                      width="40px"
                      alt="sarika jassal"
                      height="40px"
                      title=""
                      loading="lazy"
                    >
                    <h3>
                      <a href="https://wellnessextract.com/pages/dr-felicia">
                        <span class="blog-author">
                          <span><i>Reviewed By: </i> Dr. Felicia</span>
                          {% comment %} <span class="by-auth">Editor</span>  {% endcomment %}
                        </span>
                      </a>
                    </h3>
                  </div>
                {% elsif tag == 'lehri' %}
                  <div class="blog-editor">
                    <img
                      class="lazyload"
                      data-src="https://cdn.shopify.com/s/files/1/0735/8172/3944/files/Lehri.jpg?v=1741159628"
                      width="40px"
                      alt="sarika jassal"
                      height="40px"
                      title=""
                      loading="lazy"
                    >
                    <h3>
                      <a href="https://wellnessextract.com/pages/dr-surangama-lehri">
                        <span class="blog-author">
                          <span><i>Reviewed By: </i> Dr. Surangama Lehri</span>
                          {% comment %} <span class="by-auth">Editor</span>  {% endcomment %}
                        </span>
                      </a>
                    </h3>
                  </div>
                {% endif %}
              {% endfor %}
            </div>

            <div class="article-heading text-left d-none">
              <h2 class="title">
                {{ article.title }}
              </h2>
              {%- if show_author or show_date or blog.comments_enabled? -%}
                <div class="info">
                  {%- if show_author -%}
                    <span class="info-item author">
                      <span>{{ 'blogs.article.by' | t }}</span>
                      <span class="text">{{ article.author }}</span>
                    </span>
                  {%- endif -%}
                  {%- if show_date -%}
                    <span class="info-item date">
                      {{ article.published_at | date: '%d %b %Y' }}
                    </span>
                  {%- endif -%}
                  {%- if blog.comments_enabled? -%}
                    <a class="info-item comment" href="{{ article.url }}#comments">
                      {{ 'blogs.article.comments' | t: count: article.comments_count }}
                    </a>
                  {%- endif -%}
                </div>
              {%- endif -%}
            </div>
            {%- if article.image and show_main_img -%}
              <div class="article-image">
                <a href="{{ article.url }}" class="image">
                  <img
                    srcset="
                      {% if article.image.width >= 350 %}{{ article.image | img_url: '350x' }} 350w,{% endif %}
                      {% if article.image.width >= 700 %}{{ article.image | img_url: '700x' }} 700w,{% endif %}
                      {% if article.image.width >= 749 %}{{ article.image | img_url: '749x' }} 749w,{% endif %}
                      {% if article.image.width >= 1498 %}{{ article.image | img_url: '1498x' }} 1498w,{% endif %}
                      {% if article.image.width >= 1100 %}{{ article.image | img_url: '1100x' }} 1100w,{% endif %}
                      {% if article.image.width >= 2200 %}{{ article.image | img_url: '2200x' }} 2200w,{% endif %}
                    "
                    sizes="(min-width: 1200px) 1100px, (min-width: 750px) calc(100vw - 10rem), 100vw"
                    src="{{ article.image | img_url: '1100x' }}"
                    {% unless settings.enable_layout_rtl %}
                      loading="lazy"
                    {% endunless %}
                    width="{{ article.image.width }}"
                    height="{{ article.image.height }}"
                    alt="{{ article.image.alt | escape }}"
                  >
                </a>
              </div>
            {%- endif -%}
          </div>
          <div class="article-body">
            <div class="article-content halo-text-format{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
              <div id="table-of-contents"></div>
              <style>
                div#table-of-contents{
                border: 1px solid #ddd;
                background-color: #f7fafc;
                padding: 10px;
                border-radius: 6px;
                }

                div#table-of-contents ul{
                margin-left: 50px;
                }

                .halo-article-content {
                max-width: 950px;
                }

                div#table-of-contents h3{
                margin: 0px;
                padding: 0px;
                }

                @media (max-width: 1024px) {
                .header-with-sidebar {
                display: flex;
                flex-direction: row-reverse !important;
                }
                }
                @media (min-width: 1024.1px) {
                .header-with-sidebar {
                display: none !important;
                }
                }

                .filter-btn-mob-details{
                border: 1px solid #006a91;
                border-radius: 4px;
                padding: 6px 10px;
                color: #006a91;
                }
                @media (max-width: 500px) {
                .author-top {
                align-items: start;
                flex-direction: column;
                gap: 12px !important;
                align-items: start !important;
                }
                }
                .blog-author-details {
                display: flex;
                gap: 10px;
                align-items: center;
                }
                .blog-editor {
                display: flex;
                gap: 12px;
                align-items: center;
                }
                .article-info-details {
                margin-top: 20px;
                margin-bottom: 15px;
                font-style: italic;
                  display: flex;
                align-items: center;
                gap: 10px;
                }
                .article-info-details p {
                margin: 5px 0;
                font-size: 16px;
                color: #999999;
                }
                .article-info-details .highlight {
                font-weight: bold;
                color: #999999;
                }
              </style>

              {% assign word_count = article.content | strip_html | split: ' ' | size %}
              {% assign reading_time = word_count | divided_by: 200 %}
              {% if word_count > 0 and reading_time == 0 %}
                {% assign reading_time = 1 %}
              {% endif %}

              <div class="article-info-details">
                {% comment %} <p>Total Words: <span class="highlight">{{ word_count }}</span></p> {% endcomment %}
                <p>
                  Estimated Reading Time:
                  <span class="highlight">
                    {{- reading_time }} minute{% if reading_time > 1 %}s{% endif -%}
                  </span>
                </p>
              </div>

              {{ article.content }}
            </div>
            {%- for block in section.blocks -%}
              {%- if block.type == 'image_gallery' -%}
                {%- liquid
                  assign swipe = block.settings.image_gallery_swipe_on_mobile
                  assign column = block.settings.column
                  assign column_mb = block.settings.column_mb
                  assign type_image_gallery = block.settings.type_image_gallery
                  assign image_gallery_title = block.settings.image_gallery_title
                  assign color_title = block.settings.color_title
                  assign fontsize_title = block.settings.fontsize_title
                  assign fontsize_title_mb = block.settings.fontsize_title_mb
                  assign margin_bottom_title = block.settings.margin_bottom_title

                  assign lookbook_style = block.settings.lookbook_style
                  assign dot_desktop_size = block.settings.dot_desktop_size | append: 'px'
                  assign dot_mobile_size = block.settings.dot_mobile_size | append: 'px'
                  assign dot_shadow_color = block.settings.dot_shadow_color | color_modify: 'alpha', 0.5
                  assign dot_outer_color = block.settings.dot_outer_color
                  assign show_dot_number = block.settings.show_dot_number
                  assign show_dot_number_mobile = block.settings.show_dot_number_mobile
                  assign dot_inner_color = block.settings.dot_inner_color
                  assign number_desktop_size = block.settings.number_desktop_size | append: 'px'
                  assign number_mobile_size = block.settings.number_mobile_size | append: 'px'
                  assign enable_lookbook_width = block.settings.enable_lookbook_width
                  assign lookbook_width = block.settings.lookbook_width | append: 'px'
                  assign lookbook_truncate = block.settings.lookbook_truncate
                  assign show_plus_icon = block.settings.show_plus_icon
                  assign item_distance = block.settings.item_distance
                -%}
                <div
                  class="articleGallery-block articleGallery-{{ section.id }}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}"
                  id="articleGallery-{{ section.id }}-{{ block.id }}"
                >
                  {% if image_gallery_title != blank %}
                    <div
                      class="halo-block-header text-left"
                      style="
                                                                                                                                                                                                                                                                                                                                                                                                    --margin-bottom-title: {{ margin_bottom_title | append: 'px' }}
                      "
                    >
                      <h3
                        class="title"
                        style="
                          --color-title: {{ color_title }};
                          --fontsize-title: {{ fontsize_title | append: 'px' }};
                          --fontsize_title_mb: {{ fontsize_title_mb | append: 'px' }};
                        "
                      >
                        <span class="text">
                          {{ image_gallery_title }}
                        </span>
                      </h3>
                    </div>
                  {% endif %}
                  <div
                    class="
                      halo-row column-{{ column }}
                      {% if swipe == 'slider' %} articleGallery-slider{% endif %}
                      {% if item_distance == 0 %} not-distance{% endif %}
                      {% if swipe == 'scroll' %} scroll-snap-mobile disable-srollbar{% endif %}
                      {% if column_mb == '2' %} column-mb-2{% endif %}
