.scientific-section {
  padding: 50px 0px;
  text-align: center;
  background: linear-gradient(to bottom, #ffffff, #fef9f2);
}

.scientific-header {
  margin-bottom: 40px;
}

#scientifically-backed .scientific-title {
    letter-spacing: 0.68px;
    text-transform: capitalize;
    color: #086B92;
    text-align: center;
    font-family: Nunito;
    font-size: 40px;
    font-style: normal;
    font-weight: 600;
    line-height: 48px;
}

#scientifically-backed .scientific-subtitle {
    color: #333;
    max-width: 700px;
    margin: 0 auto;
    font-weight: 400;
    font-size: 20px;
    line-height: 1.3;
    letter-spacing: 0.32px;
    text-align: center;
    vertical-align: middle;
}

.scientific-slider {
  margin: 0 auto;
}

.scientific-card {
  background-color: #f4f0eb;
  border-radius: 12px;
  padding: 30px 24px;
  margin: 10px;
  text-align: left;
  position: relative;
  min-height: 250px;
}

.scientific-quote-icon {
  font-size: 48px;
  color: #c0bdb9;
}

.scientific-quote-text {
    color: #3f3f3f;
    margin: 15px 0;
    font-weight: 500;
    font-size: 22px;
    line-height: 1.2;
    letter-spacing: -0.63px;
}

.scientific-verified {
  font-size: 14px;
  color: #444;
}

.scientific-icon {
  margin-right: 6px;
}

.scientific-note {
  display: block;
  font-size: 10px;
  color: #888;
  margin-top: 4px;
}

/* Slick Dots */
.slick-dots {
  margin-top: 30px;
}

.slick-dots li button:before {
  color: #00587A;
}
  @media(max-width: 1025px){
