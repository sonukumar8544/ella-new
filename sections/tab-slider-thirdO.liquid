   <style>
        .custom-container {
            overflow: hidden;
        }

        .content {
            display: flex;
        }

        .tabs-sidebar {
            flex: 0 0 400px;
            padding: 0px 20px 0px 0px;
        }

        .tab-button {
            display: block;
            width: 100%;
            padding: 15px 20px;
            margin-bottom: 10px;
            background: white;
            border: 2px solid #e9ecef;
            border-radius: 12px;
            font-size: 20px;
            color: #3f3f3f;
            cursor: pointer;
            transition: all 0.3s ease;
            text-align: left;
            font-weight: 500;
        }

        .tab-button:hover {
            border-color: #086B92;
            color: #086B92;
            transform: translateX(5px);
        }

        .tab-button.active {
            background: #086B92;
            border-color: #086B92;
            color: white;
            transform: translateX(5px);
        }

        .tab-content {
            flex: 1;
            position: relative;
            overflow: hidden;
        }

        .tab-panel {
            display: none !important;
            width: 100%;
            height: 100%;
            opacity: 0;
            transform: translateX(50px);
            transition: all 0.4s ease;
            pointer-events: none;
            display: flex;
            flex-direction: column;
        }

        .tab-panel.active {
            opacity: 1;
            transform: translateX(0);
            pointer-events: all;
            display: flex !important;
        }

        .swipe-hint {
            position: absolute;
            bottom: 10px;
            right: 20px;
            color: rgba(255, 255, 255, 0.8);
            font-size: 0.9rem;
            font-style: italic;
        }

        .content-secton {
            position: absolute;
            inset: 0;
            padding: 30px 30px 60px;
        }

        .inner-items {
            position: relative;
            border-radius: 16px;
            overflow: hidden;
            background-color: #EEEEEE;
        }

        .inner-items.second-itmes img {
            opacity: 0;
        }

        .inner-content {
            display: flex;
            flex-direction: column;
            justify-content: center;
            justify-self: flex-start;
            align-self: center;
            height: 100%;
            gap: 20px;
            max-width: 60%;
        }

        h2.slider-title {
            font-size: 40px;
            color: #086B92;
            margin: 0;
            line-height: 49px;
            letter-spacing: 0;
        }

        .inner-content p {
            font-size: 22px;
            color: #3F3F3F;
            line-height: 32px;
            margin: 0px;
        }

        .title-group p {
            font-size: 25px;
        }

        .content-btn {
            display: inline-block;
            font-size: 24px;
            font-weight: 500;
            font-style: italic;
            color: #3F3F3F;
        }

        .inner-items.second-itmes .inner-content {
            max-width: 100%;
        }

        .solution-btn.button {
            margin: 0px;
            display: inline-block;
            width: unset;
            min-width: max-content;
            max-width: max-content;
            background: #086B92;
            border-color: #086B92;
            color: #fff;
        }

        a.solution-btn.button:hover {
            background: #f1d074;
            border-color: #f1d074;
            color: #086B92;
        }

        .inner-items img {
            min-height: 300px;
            object-fit: cover;
            object-position: 78%;
        }

        @media (max-width: 768px) {
            .tab-button {
                padding: 10px 15px;
                border-radius: 10px;
                font-size: 16px;
            }

            .content {
                flex-direction: column;
            }

            .tabs-sidebar {
                flex: none;
                display: flex;
                overflow-x: auto;
                padding: 10px;
                gap: 10px;
                padding-left: 0px;
                margin-bottom: 15px;
            }

            .tab-button {
                flex: 0 0 auto;
                white-space: nowrap;
                margin-bottom: 0;
                max-width: max-content;
            }
        }

        @media (max-width: 767.8px) {
            .content-secton {
                position: absolute;
                inset: 0;
                padding: 10px 15px 30px;
            }

            .inner-content {
                max-width: 85%;
                gap: 10px;
            }

            h2.slider-title {
                font-size: 24px;
                line-height: 28px;
            }

            .title-group p {
                font-size: 17px;
            }

            .inner-content p {
                font-size: 15px;
                line-height: 20px;
            }

            .content-btn {
                font-size: 14px;
                font-weight: 600;
            }
        }

        @media (max-width: 480px) {
            .tab-button {
                padding: 8px 12px;
                font-size: 14px;
            }
        }

        @media (max-width: 350px) {
            h2.slider-title {
                font-size: 20px;
                line-height: 24px;
            }
        }

